<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/activity_uc__btn_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/activity_uc__btn_3"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_margin="50dp"
        app:srcCompat="@drawable/ic_list"
        app:backgroundTint="@color/apicture"
        android:onClick="onClickHome"
        />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/activity_uc__btn_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/activity_uc__btn_1"
        android:layout_margin="50dp"
        app:backgroundTint="@color/apicture"
        app:srcCompat="@android:drawable/ic_input_add"
        android:onClick="onClickCollections"
        />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_options"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="@color/apicture"
        android:clickable="true"
        android:focusable="true"
        android:tint="@android:color/white"
        app:fabCustomSize="65dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:maxImageSize="35dp"
        app:srcCompat="@drawable/ic_baseline_settings_24"
        tools:ignore="MissingConstraints,VectorDrawableCompat" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/activity_uc__btn_0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:fabCustomSize="100dp"
        app:layout_constraintTop_toTopOf="@+id/fab_options"
        app:layout_constraintBottom_toBottomOf="@+id/fab_options"
        app:layout_constraintStart_toStartOf="parent"

        android:layout_margin="25dp"
        app:srcCompat="@drawable/ic_person"
        app:backgroundTint="#3E3838"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/activity_uc__tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="VR009"
        android:textSize="22dp"
        app:layout_constraintStart_toEndOf="@+id/activity_uc__btn_0"
        app:layout_constraintBottom_toBottomOf="@+id/activity_uc__btn_0"
        app:layout_constraintTop_toTopOf="@+id/activity_uc__btn_0"
        android:layout_marginLeft="10dp"
        />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/activity_uc__rv_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:text="@string/default_collection"
        android:id="@+id/activity_uc__el_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/apicture"
        android:minHeight="100dp"
        android:minWidth="280dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_uc__btn_0"
        android:layout_marginTop="40dp"
        android:onClick="collectionOpen"
        />

</androidx.constraintlayout.widget.ConstraintLayout>